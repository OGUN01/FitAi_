# FitAI Environment Configuration
# Copy this file to .env and fill in your actual values

# Google Gemini AI API Key
# Get your API key from: https://makersuite.google.com/app/apikey
EXPO_PUBLIC_GEMINI_API_KEY=your_gemini_api_key_here

# Supabase Configuration (for Chat 1 - Backend)
EXPO_PUBLIC_SUPABASE_URL=your_supabase_url_here
EXPO_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# Optional: Additional API Keys for enhanced features
# EXPO_PUBLIC_NUTRITION_API_KEY=your_nutrition_api_key_here
# EXPO_PUBLIC_EXERCISE_API_KEY=your_exercise_api_key_here

# Development Settings
NODE_ENV=development
EXPO_PUBLIC_APP_ENV=development

# Analytics (Optional)
# EXPO_PUBLIC_ANALYTICS_ID=your_analytics_id_here
