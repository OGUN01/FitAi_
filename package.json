{
  "name": "fitai",
  "version": "0.1.6",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "dev:start": "expo start --dev-client",
    "dev:android": "expo start --dev-client --android",
    "dev:ios": "expo start --dev-client --ios",
    "dev:tunnel": "expo start --dev-client --tunnel",
    "dev:build": "eas build --platform android --profile development",
    "dev:install": "adb install development-build.apk",
    "dev:setup": "node dev-setup.js",
    "dev:status": "node dev-setup.js status",
    "android:setup": "node android-emulator-setup.js",
    "android:list": "node android-emulator-setup.js list",
    "android:start": "node android-emulator-setup.js start",
    "android:install": "node android-emulator-setup.js install",
    "android:status": "node android-emulator-setup.js status",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:onboarding": "jest src/__tests__/onboarding/",
    "test:onboarding:unit": "jest src/__tests__/onboarding/OnboardingContainer.test.tsx",
    "test:onboarding:integration": "jest src/__tests__/onboarding/OnboardingFlow.integration.test.tsx",
    "test:onboarding:e2e": "jest src/__tests__/e2e/onboarding.e2e.test.tsx",
    "test:onboarding:all": "node scripts/run-onboarding-tests.js --full",
    "test:onboarding:quick": "node scripts/run-onboarding-tests.js --quick",
    "test:onboarding:device": "node scripts/run-onboarding-tests.js --emulator --full",
    "test:gemini": "node test-gemini-simple.js",
    "test:gemini-full": "node test-gemini-structured-output.js",
    "build:development-local": "eas build --platform android --profile development --local",
    "build:preview": "eas build --platform android --profile preview",
    "build:preview-local": "eas build --platform android --profile preview-local --local",
    "build:production": "eas build --platform android --profile production",
    "build:production-local": "eas build --platform android --profile production --local",
    "build:production-aab": "eas build --platform android --profile production-aab",
    "build:production-aab-local": "eas build --platform android --profile production-aab --local",
    "submit:production": "eas submit --platform android"
  },
